{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "rewrites": [
                {
                    "source": "/api/(.*)",
                    "destination": "/backend/app.js"
                }
            ]
        }
    ]
}