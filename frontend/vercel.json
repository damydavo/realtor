{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/.*",
            "dest": "package.json"
        }
    ]
}